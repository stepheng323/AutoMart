{
	"name": "Automart",
	"version": "1.0.0",
	"description": "API endpoints to power front end pages",
	"main": "app.js",
	"scripts": {
		"start": "sudo nodemon app.js --exec babel-node --",
		"serve": "node ./build/app.js",
		"build": "npm run clean && babel . --ignore node_modules --out-dir ./build",
		"clean": "rm -rf build && mkdir build",
		"test": " nyc mocha --exit --require @babel/register",
		"coverage": "nyc report --reporter=text-lcov | coveralls"
	},
	"keywords": [
		"API",
		"endpoints"
	],
	"author": "Abiodun Oyebanji",
	"license": "ISC",
	"dependencies": {
		"bcrypt": "^3.0.6",
		"cloudinary": "^1.14.0",
		"dotenv": "^8.0.0",
		"express": "^4.17.0",
		"joi": "^14.3.1",
		"jsonwebtoken": "^8.5.1",
		"multer": "^1.4.1",
		"pg": "^7.11.0"
	},
	"devDependencies": {
		"@babel/cli": "^7.4.4",
		"@babel/core": "^7.4.5",
		"@babel/node": "^7.4.5",
		"@babel/preset-env": "^7.4.5",
		"@babel/register": "^7.4.4",
		"chai": "^4.2.0",
		"chai-http": "^4.3.0",
		"coveralls": "^3.0.3",
		"eslint": "^5.16.0",
		"eslint-config-airbnb-base": "^13.1.0",
		"eslint-plugin-import": "^2.17.3",
		"mocha": "^6.1.4",
		"nodemon": "^1.19.0",
		"nyc": "^14.1.1"
	}
}
